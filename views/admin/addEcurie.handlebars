<div class="mycenter">
  <h1>Ajouter une écurie</h1>
  <form action="#" method="post" enctype="multipart/form-data">
    <p>
      <label for="ecunom">Nom</label>
      <br/>
      <input type="text" name="ecunom" id="ecunom" required>
    </p>

    <p>
      <label for="ecunomdir">Nom directeur</label>
      <br/>
      <input type="text" name="ecunomdir" id="ecunomdir" required>
    </p>
    <p>
      <label for="ecuadrsiege">Adresse siège</label>
      <br/>
      <input type="text" name="ecuadrsiege" id="ecuadrsiege" required>
    </p>
    <p>
      <label for="ecupoints">Nombre de points</label>
      <br/>
      <input type="number" name="ecupoints" id="ecupoints" required>
    </p>
    <p>
      <label for="paynum">Pays</label>
      <br/>
      <select id="paynum" name="paynum">
        {{#each pays}}
          <option value="{{this.paynum}}">{{this.paynom}}</option>
        {{/each}}
      </select>
    </p>

    <p>
      <label for="ecuadresseimage">Image</label>
      <br/>
      <input type="file" name="ecuadresseimage" id="ecuadresseimage" multiple="multiple">
    </p>



    <input type="submit" value="Ajouter">

  </form>
</div>
